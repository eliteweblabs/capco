      precision mediump float;
      uniform vec2 u_resolution;
      uniform float u_time;

      // hash
      float hash(float n) {
        return fract(sin(n) * 43758.5453);
      }

      // 2D noise function
      float noise(vec2 p) {
        vec2 i = floor(p);
        vec2 f = fract(p);
        vec2 u = f * f * (3.0 - 2.0 * f);
        return mix(mix(hash(i.x + hash(i.y)), hash(i.x + 1.0 + hash(i.y)), u.x),
                   mix(hash(i.x + hash(i.y + 1.0)), hash(i.x + 1.0 + hash(i.y + 1.0)), u.x), u.y);
      }

      // mountain range
      float mountainRange(vec2 uv) {
        float mountainHeight = 0.0;
        float frequency = 2.0;
        float amplitude = 0.5;
        for (int i = 0; i < 5; i++) {
          mountainHeight += noise(uv * frequency) * amplitude;
          frequency *= 2.0;
          amplitude *= 0.5;
        }
        return mountainHeight;
      }

      // HSV to RGB conversion
      vec3 hsv2rgb(vec3 c) {
        vec4 K = vec4(1.0, 2.0 / 3.0, 1.0 / 3.0, 3.0);
        vec3 p = abs(fract(c.xxx + K.xyz) * 6.0 - K.www);
        return c.z * mix(K.xxx, clamp(p - K.xxx, 0.0, 1.0), c.y);
      }

      // movement
      vec3 auroraLayer(vec2 uv, float speed, float intensity, float hueOffset) {
        float t = u_time * speed;
        vec2 scaleXY = vec2(2.0, 2.0);
        vec2 movement = vec2(2.0, -2.0);
        vec2 p = uv * scaleXY + t * movement;
        float n = noise(p + noise(vec2(hueOffset) + p + t));
        
        float aurora = (n - uv.y * 0.8);
        aurora = max(aurora, -0.05);
        
        // spectrum
        float hue = uv.x + uv.y * 0.6 + sin(t * 0.7 + hueOffset) * ${e.hue_noise_influence.toFixed(2)};
        hue = fract(hue);
        hue = ${e.hue_base.toFixed(2)} + sin(hue * 6.28318) * ${e.hue_variation.toFixed(2)}; // oscillate between cyan to blue, less purple
        
        vec3 color = hsv2rgb(vec3(hue, ${e.saturation.toFixed(2)}, ${e.brightness.toFixed(2)}));
        
        return color * aurora * intensity * 1.6;
      }

      void main() {
        vec2 uv = gl_FragCoord.xy / u_resolution.xy;
        uv.x *= u_resolution.x / u_resolution.y;

        // Apply pixelation effect
        vec2 pixelatedUV = uv;
        if (${e.pixel_size.toFixed(1)} > 1.0) {
          vec2 pixelSize = vec2(${e.pixel_size.toFixed(1)}) / u_resolution.xy;
          pixelatedUV = floor(uv / pixelSize) * pixelSize;
        }

        vec3 backgroundColor = vec3(0.075,0.075,0.09);
        vec3 auroraColor = vec3(0.075,0.075,0.09);

        auroraColor += auroraLayer(pixelatedUV, ${e.layer1_speed.toFixed(3)}, ${e.layer1_intensity.toFixed(2)}, 0.0);
        auroraColor += auroraLayer(pixelatedUV, ${e.layer2_speed.toFixed(3)}, ${e.layer2_intensity.toFixed(2)}, 1.0);
        auroraColor += auroraLayer(pixelatedUV, ${e.layer3_speed.toFixed(3)}, ${e.layer3_intensity.toFixed(2)}, 2.0);
        auroraColor += auroraLayer(pixelatedUV, ${e.layer4_speed.toFixed(3)}, ${e.layer4_intensity.toFixed(2)}, 3.0);
        
        // alpha
        float auroraAlpha = length(auroraColor) * ${e.alpha_multiplier.toFixed(2)};
        auroraAlpha = clamp(auroraAlpha, 0.0, 1.0);

        // blend
        vec3 finalColor = mix(backgroundColor, backgroundColor + auroraColor, auroraAlpha);

        // pixel intensity
        vec3 originalColor = finalColor;
        if (${e.pixel_size.toFixed(1)} > 1.0) {
          // Sample original non-pixelated version for blending
          vec3 nonPixelatedAurora = vec3(0.075,0.075,0.09);
          nonPixelatedAurora += auroraLayer(uv, ${e.layer1_speed.toFixed(3)}, ${e.layer1_intensity.toFixed(2)}, 0.0);
          nonPixelatedAurora += auroraLayer(uv, ${e.layer2_speed.toFixed(3)}, ${e.layer2_intensity.toFixed(2)}, 1.0);
          nonPixelatedAurora += auroraLayer(uv, ${e.layer3_speed.toFixed(3)}, ${e.layer3_intensity.toFixed(2)}, 2.0);
          nonPixelatedAurora += auroraLayer(uv, ${e.layer4_speed.toFixed(3)}, ${e.layer4_intensity.toFixed(2)}, 3.0);
          
          float nonPixelatedAlpha = length(nonPixelatedAurora) * ${e.alpha_multiplier.toFixed(2)};
          nonPixelatedAlpha = clamp(nonPixelatedAlpha, 0.0, 1.0);
          vec3 nonPixelatedFinal = mix(backgroundColor, backgroundColor + nonPixelatedAurora, nonPixelatedAlpha);
          
          finalColor = mix(nonPixelatedFinal, finalColor, ${e.pixel_intensity.toFixed(2)});
        }

        gl_FragColor = vec4(finalColor, 1.0);
      }
