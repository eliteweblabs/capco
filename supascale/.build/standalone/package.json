{
  "name": "supascale-web",
  "version": "1.1.33",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "version": "node scripts/generate-version.js",
    "prebuild": "npm run version",
    "build": "next build",
    "build:standalone": "npm run version && next build && npm run copy-assets",
    "copy-assets": "cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/",
    "start": "next start",
    "start:standalone": "node .next/standalone/server.js",
    "lint": "next lint",
    "release": "./scripts/build-release.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.971.0",
    "@azure/storage-blob": "^12.30.0",
    "@google-cloud/storage": "^7.18.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.6.2",
    "clsx": "^2.1.1",
    "cron-parser": "^5.5.0",
    "crypto-js": "^4.2.0",
    "lucide-react": "^0.469.0",
    "next": "^15.1.0",
    "next-auth": "^5.0.0-beta.25",
    "node-cron": "^3.0.3",
    "pm2": "^6.0.14",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "recharts": "^3.6.0",
    "supashield": "^0.2.1",
    "systeminformation": "^5.30.5",
    "tailwind-merge": "^2.6.0",
    "yaml": "^2.8.2",
    "zod": "^3.24.1"
