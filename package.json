{
  "name": "astro-supabase-main",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run kill-servers && astro dev --port 4321",
    "kill-servers": "node scripts/cleanup-servers.js",
    "cleanup": "npm run kill-servers",
    "start": "astro dev",
    "build": "astro build --skip-check",
    "build:railway": "NODE_OPTIONS='--max-old-space-size=8192' astro build --skip-check",
    "preview": "astro preview",
    "astro": "astro",
    "check-db-fields": "node check-database-fields.js",
    "check-sql-functions": "node check-sql-functions.js",
    "convert-fields": "node automate-field-conversion.js",
    "convert-fields-apply": "node automate-field-conversion.js --apply",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,astro,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,astro,css,md,json}\"",
    "lint": "eslint \"src/**/*.{js,ts,astro}\" --report-unused-disable-directives",
    "lint:fix": "eslint \"src/**/*.{js,ts,astro}\" --fix",
    "check": "astro check && tsc --noEmit",
    "validate": "npm run format:check && npm run lint && npm run check"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/node": "^9.0.0",
    "@astrojs/tailwind": "^5.1.1",
    "@fontsource-variable/outfit": "^5.2.8",
    "@supabase/supabase-js": "^2.74.0",
    "@tailwindcss/forms": "^0.5.10",
    "aos": "^2.3.4",
    "astro": "^5.0.0",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "flowbite": "^3.1.2",
    "node-fetch": "^3.3.2",
    "puppeteer": "^24.23.0",
    "stripe": "^19.1.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/aos": "^3.0.7",
    "@types/micromatch": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1"
  }
}
