{
  "name": "astro-supabase-main",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "generate-global-functions": "tsx scripts/scan-global-functions.ts",
    "generate-colors": "node scripts/generate-colors.js",
    "prepare-favicons": "node scripts/prepare-favicons.js",
    "process-manifest": "node scripts/process-manifest.js",
    "dev": "npm run prepare-favicons && npm run build:pdf-extractor && npm run kill-servers && NODE_ENV=development astro dev --port 4321 --host",
    "open:dev": "open http://127.0.0.1:4321",
    "check:port80": "bash scripts/check-port-80.sh",
    "dev:sync": "npm run prepare-favicons && npm run kill-servers && concurrently -n \"ASTRO,BROWSER-SYNC\" -c \"blue,green\" \"astro dev --port 4321 --host\" \"browser-sync start --config bs-config.cjs\"",
    "dev:tunnel": "npm run prepare-favicons && npm run kill-servers && node scripts/start-dev-with-tunnel-fixed.js",
    "kill-servers": "node scripts/cleanup-servers.js",
    "cleanup": "npm run kill-servers",
    "start": "node dist/server/entry.mjs",
    "preview:prod": "npm run build && PORT=3000 npm run start",
    "newsletter:scheduler": "tsx scripts/newsletter-scheduler.ts",
    "build": "npm run prepare-favicons && npm run process-manifest && npm run build:app-init && npm run build:app-globals && npm run build:auth-google && npm run build:app-page-scripts && npm run build:pdf-extractor && astro build --skip-check",
    "build:railway": "npm run prepare-favicons && npm run process-manifest && npm run generate-colors && npm run generate-global-functions && npm run build:multistep-form && npm run build:typewriter && npm run build:app-init && npm run build:app-globals && npm run build:auth-google && npm run build:app-page-scripts && NODE_OPTIONS='--max-old-space-size=8192' astro build --skip-check",
    "build:multistep-form": "node scripts/build-multistep-form-standalone.mjs",
    "build:typewriter": "node scripts/build-typewriter-standalone.mjs",
    "build:app-init": "node scripts/build-app-init-standalone.mjs",
    "build:app-globals": "node scripts/build-app-globals-standalone.mjs",
    "build:auth-google": "node scripts/build-auth-google-standalone.mjs",
    "build:app-page-scripts": "node scripts/build-app-page-scripts.mjs",
    "build:pdf-extractor": "node scripts/build-pdf-extractor-standalone.mjs",
    "update-vapi": "node scripts/update-vapi-from-master.js all",
    "update-vapi:capco": "node scripts/update-vapi-from-master.js capco",
    "update-vapi:innercity": "node scripts/update-vapi-from-master.js innercity",
    "update-vapi-capco": "tsx scripts/vapi-capco-config.js",
    "update-vapi-innercity": "tsx scripts/vapi-innercity-fireprotection-config.js",
    "test:vapi": "node scripts/test-vapi-booking.js",
    "vapi:logs": "node scripts/get-vapi-logs.js",
    "test:cert": "tsx scripts/test-certificate.ts",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,astro,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,astro,css,md,json}\"",
    "lint": "eslint \"src/**/*.{js,ts,astro}\" --report-unused-disable-directives",
    "lint:fix": "eslint \"src/**/*.{js,ts,astro}\" --fix",
    "check": "astro check && tsc --noEmit",
    "monitor:errors": "node scripts/monitor-errors.js",
    "trim:icons": "node scripts/trim-icon-data.cjs",
    "validate": "npm run format:check && npm run lint && npm run check"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@astrojs/check": "^0.8.3",
    "@astrojs/node": "8.3.4",
    "@astrojs/react": "^3.6.0",
    "@astrojs/tailwind": "^5.1.1",
    "@floating-ui/dom": "^1.7.5",
    "@fontsource-variable/outfit": "^5.2.8",
    "@signpdf/placeholder-plain": "^3.2.5",
    "@signpdf/signer-p12": "^3.2.4",
    "@signpdf/signpdf": "^3.2.5",
    "@signpdf/utils": "^3.2.4",
    "@supabase/supabase-js": "^2.74.0",
    "@tailwindcss/forms": "^0.5.10",
    "@types/node-cron": "^3.0.11",
    "@types/node-forge": "^1.3.14",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@xyflow/react": "^12.8.6",
    "aos": "^2.3.4",
    "astro": "4.15.12",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "cropperjs": "^2.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "flowbite": "^3.1.2",
    "googleapis": "^171.0.0",
    "gray-matter": "^4.0.3",
    "libphonenumber-js": "^1.12.35",
    "marked": "^17.0.1",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "node-forge": "^1.3.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^2.4.3",
    "pdf-poppler": "^0.2.3",
    "pg": "^8.16.3",
    "puppeteer": "^24.23.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-flow": "^1.0.3",
    "sharp": "^0.34.5",
    "stripe": "^19.1.0",
    "studiocms": "^0.1.0-beta.31",
    "swiper": "^12.1.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tesseract.js": "^6.0.1",
    "typeit": "^8.8.7",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/aos": "^3.0.7",
    "@types/marked": "^5.0.2",
    "@types/micromatch": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "baseline-browser-mapping": "^2.9.19",
    "browser-sync": "^3.0.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-css-order": "^2.2.0",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tsx": "^4.19.0"
  },
  "overrides": {
    "crypto-js": "^4.2.0"
  }
}
