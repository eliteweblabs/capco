---
import FlowbiteNavbar from "./FlowbiteNavbar.astro";

const { isAuth, currentUser, currentRole, session, desktopNavigationHTML, isBackend } = Astro.props;

const { id } = Astro.params;
---

<FlowbiteNavbar
  isAuth={isAuth}
  currentUser={currentUser}
  currentRole={currentRole}
  session={session}
  desktopNavigationHTML={desktopNavigationHTML}
  id={id}
  isBackend={isBackend}
/>
